{
    "name": "dicr/yii2-log",
    "type": "yii2-extension",
    "license": "GPL-3.0-or-later",
    "description": "Logging components for Yii2",
    "keywords": ["dicr", "php", "yii2", "log"],
    "homepage": "https://gitlab.com/dicr/yii2-log",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "https://dicr.org",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-log.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\log\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~7.4 || ~8.0.1",
        "ext-mbstring": "*",

        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap5": "~2.0",

        "dicr/yii2-asset": "~2.0",
        "dicr/yii2-helper": "~5.0",
        "dicr/yii2-site": "~8.0",
        "dicr/yii2-validate": "~2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"},
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-asset.git"},
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-helper.git"},
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-site.git"},
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-validate.git"}
    ]
}
